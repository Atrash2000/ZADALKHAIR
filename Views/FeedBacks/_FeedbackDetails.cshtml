@model ZADALKHAIR.Models.FeedBack

<div class="modal-header">
    <div class="row px-2" style="flex-direction: column">
        <h3 style="color: black !important;" class="modal-title" id="exampleModalLongTitle">@Model.Name</h3>
        <span style="color: black !important;">@Model.Email</span><br />
        <span style="color: black !important;">@Model.CreatedAt.ToString().Split(" ")[0]</span>
    </div>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<div class="modal-body" style="color: black !important;">
    @Model.Massege
</div>
<div class="modal-footer" style="color: black !important;">
    <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
</div>
