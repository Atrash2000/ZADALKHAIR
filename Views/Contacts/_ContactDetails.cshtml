@model ZADALKHAIR.Models.Contact

<div class="modal-header">
    <div class="row px-2" style="flex-direction: column">
        <h3 class="modal-title" id="exampleModalLongTitle"  style="color: black !important;">@Model.ContactSubject</h3>
        <span  style="color: black !important;">@Model.ContactEmail</span><br />
        <span  style="color: black !important;">@Model.CreatedAt.ToString().Split(" ")[0]</span>
    </div>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<div class="modal-body" style="color: black !important;">
    @Model.ContactMassege
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
</div>
