<body onscroll="controllNavBar()">

    <div class="spinner">
        <div class="center">
            <div class="ring"></div>
            <span>loading...</span>
        </div>
    </div>

    <header>
        <div class="sidebar-bg"></div>
        <aside class="sidebar px-4 py-3">
            <button type="button" onclick="closeSidebar()" class="close btn-close text-white position-fixed" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
            <nav>
                <ul>
                    <li class="mb-3">
                        <a asp-action="Index" asp-controller="Home" class="text-white">Home</a>
                    </li>
                    <li class="mb-3">
                        <a asp-action="AboutUs" asp-controller="Home" class="text-white">About Us</a>
                    </li>
                    <li class="mb-3">
                        <a asp-action="ServiceView" asp-controller="Home" class="text-white">Our Services</a>
                    </li>
                    <li class="mb-3">
                        <a asp-action="Create" asp-controller="Contacts" class="text-white">Contact Us</a>
                    </li>
                    <li class="mb-3">
                        <a asp-action="Login" asp-controller="Users" class="text-white">Login</a>
                    </li>
                </ul>
            </nav>
        </aside>

        <nav class="fixed-top bg-transparent">
            <div class="container-fluid d-flex justify-content-between w-100" id="nav-container">
                <img src="~/images/logo.png" class="logo-img-1" alt="logo" id="logo">

                <ul class="d-flex">

                    <li class="pt-4 mx-4">
                        <div onclick="showSideBar()" class="hamburger">
                            <div class="top-bun"></div>
                            <div class="meat"></div>
                            <div class="bottom-bun"></div>
                        </div>
                    </li>
                </ul>
            </div>

        </nav>
    </header>

    <main role="main" class="pb-3">
        @RenderBody()
    </main>


